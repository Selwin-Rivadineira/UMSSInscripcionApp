    "macaddress": "^0.5.3",
    "node-stream-zip": "^1.15.0",
    "reflect-metadata": "^0.2.2",
    "simple-git": "^3.23.0",
    "tar": "^7.4.3",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "uuid4": "^2.0.3",
    "which": "^5.0.0",
    "winreg": "^1.2.5",
    "winston": "^3.17.0",
