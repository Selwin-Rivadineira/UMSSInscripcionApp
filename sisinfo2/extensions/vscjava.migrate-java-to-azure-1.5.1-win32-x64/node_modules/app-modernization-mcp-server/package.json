{
  "name": "app-modernization-mcp-server",
  "version": "1.5.1",
  "description": "MCP server using modelcontextprotocol/sdk with STDIO protocol.",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./tools": "./dist/core/index.js",
    "./types": "./dist/types.js",
    "./ioc_for_ext": "./dist/core/ioc_for_ext.js"
  },
  "files": [
    "dist/"
  ],
  "aiKey": "b73649cf-ca8f-4768-9e0e-b789d8529db5",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier --write \"**/*.{js,ts,md}\"",
    "pack:tgz": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true });\" && tsc -p ./tsconfig.pack.json && npm pack",
    "build": "npm run lint && webpack --mode development",
    "package": "npm run lint && webpack --mode production",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "inspector": "npx -y @modelcontextprotocol/inspector ts-node src/index.ts",
    "test": "npx jest --testMatch \"<rootDir>/src/**/*.test.ts\" --passWithNoTests --coverage",
    "test:integration": "npm run package && npx jest --testMatch \"<rootDir>/tests/**/*.test.ts\" --passWithNoTests --detectOpenHandles"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
