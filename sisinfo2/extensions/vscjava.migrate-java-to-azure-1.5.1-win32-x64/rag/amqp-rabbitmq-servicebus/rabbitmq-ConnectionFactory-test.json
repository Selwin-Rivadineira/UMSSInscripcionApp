{
  "filePattern": {
    "shouldMatch": [
      "RabbitConfig.java",
      "src/main/java/com/example/config/RabbitMQConfig.java",
      "nested/folder/ConnectionFactoryConfig.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "RabbitConfig.js", 
      "src/main/resources/application.properties",
      "src/main/kotlin/RabbitConfig.kt",
      "config/rabbitmq.yml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "import org.springframework.amqp.rabbit.connection.ConnectionFactory;",
      "import org.springframework.amqp.rabbit.connection.CachingConnectionFactory;",
      "rabbit.connection.ConnectionFactory factory = new rabbit.connection.ConnectionFactory();"
    ],
    "shouldNotMatch": [
      "import org.springframework.jms.JmsException;",
      "import com.azure.messaging.servicebus.ServiceBusClientBuilder;", 
      "ServiceBusClientBuilder builder = new ServiceBusClientBuilder();",
      "import org.springframework.rabbitmq.connection; // non-existent package",
      "String rabbitMQHost = \"localhost\";"
    ]
  }
}
