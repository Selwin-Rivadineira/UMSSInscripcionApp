{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "CopyObjectRequest request = CopyObjectRequest.builder().build();",
      "CopyObjectResponse response;",
      "s3Client.copyObject(request);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}