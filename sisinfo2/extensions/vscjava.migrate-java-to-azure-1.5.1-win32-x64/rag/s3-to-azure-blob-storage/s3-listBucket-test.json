{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "ListBucketsRequest req = ListBucketsRequest.builder().bucketRegion(\"us-west-2\").build();",
      "s3Client.listBuckets();",
      "ListBucketsResponse response = s3Client.listBuckets(ListBucketsRequest.builder().build());"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobServiceClient blobServiceClient = new BlobServiceClientBuilder().build();"
    ]
  }
}
