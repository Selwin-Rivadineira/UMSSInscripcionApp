{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "GetObjectRequest request = GetObjectRequest.builder().bucket(bucketName).key(objectKey).build();",
      "s3Client.getObject(request);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobClient blobClient = new BlobClient();"
    ]
  }
}
