{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "HeadBucketRequest req = HeadBucketRequest.builder().bucket(\"example-bucket\").build();",
      "HeadBucketResponse response;",
      "s3Client.headBucket(req);"
    ],
    "shouldNotMatch": [
      "BlobContainerClient client = new BlobContainerClient();",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
