{
  "filePattern": {
    "shouldMatch": [
      "CreateBucketRequest.java",
      "nested/CreateBucketRequest.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/CreateBucketRequest.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "CreateBucketRequest.Builder builder = CreateBucketRequest.builder().bucket(\"myBucket\");",
      "CreateBucketResponse response = s3Client.createBucket(builder.build());"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobContainerClient containerClient = new BlobContainerClient();"
    ]
  }
}
