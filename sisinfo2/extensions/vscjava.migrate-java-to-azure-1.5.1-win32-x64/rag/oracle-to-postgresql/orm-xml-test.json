{
  "filePattern": {
    "shouldMatch": [
      "src/Main.xml"
    ],
    "shouldNotMatch": [
      "src/Main.jav",
      "src/Main.txt",
      "src/Main.java.bak",
      "src/Main.java1",
      "src/Main.javac",
      "src/Main.py",
      "src/Main.java/extra",
      "src/.java",
      "src/Main.JAVA",
      "src/com/example/util/Helper.java",
      "src/database/OracleMigration.java",
      "src/database/OracleMigration.bak",
      "src/database/PostgreSQLMigration.bak",
      "src/com/example/orm/EntityMapping.bak",
      "src/com/example/orm/DatabaseConfig.bak"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "resultMap",
      "select",
      "insert",
      "update",
      "delete",
      "<if>",
      "<where>",
      "<foreach>",
      "<sql>",
      "<script>",
      "ROWNUM",
      "DUAL",
      "NVL",
      "CONNECT BY",
      "VARCHAR2",
      "NUMBER",
      "PL/SQL",
      "column=\"test\"",
      "table=\"test\"",
      "FROM test",
      "JOIN test"
    ],
    "shouldNotMatch": [
      "String path = \"/WEB-INF/config.xml\";",
      "String path = \"/src/main/resources/config.properties\";",
      "String url = \"https://example.com/api/v1\";",
      "String url = \"http://example.com/api/v1\";",
      "String[] urls = {\"http://example.com/api/v1\", \"https://example.org/api/v2\"};",
      "String themeProperties = \"# Definition of the \\\"High Contrast\\\" theme.\\n\" + \"styleSheet = style/hicon.css\\n\";",
      "String themeProperties = \"# Definition of the http://www.midnightfun.co.uk/ \\\"MidnightFun\\\" theme.\\n\" + \"styleSheet = style/midnightfun.css\\n\";"
    ]
  }
}
