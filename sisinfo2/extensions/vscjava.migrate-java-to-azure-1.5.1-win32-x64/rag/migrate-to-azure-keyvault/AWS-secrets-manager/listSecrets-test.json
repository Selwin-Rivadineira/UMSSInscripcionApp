{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "ListSecretsRequest request = ListSecretsRequest.builder().maxResults(10).build();",
      "ListSecretsRequest request = ListSecretsRequest.builder().includePlannedDeletion(true).build();",
      "secretsManagerClient.listSecrets(request);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "System.out.println(\"Unrelated code snippet\");"
    ]
  }
}
