{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/build.gradle",
      "nested/build.gradle.kts"
    ],
    "shouldNotMatch": [
      "src/ExampleClass.java",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>spring-cloud-starter-aws-secrets-manager-config</artifactId>",
      "<artifactId>spring-cloud-aws-secrets-manager-config</artifactId>",
      "<artifactId>spring-cloud-aws-starter-secrets-manager</artifactId>",
      "<artifactId>spring-cloud-aws-secrets-manager</artifactId>",
      "implementation 'io.awspring.cloud:spring-cloud-starter-aws-secrets-manager-config'",
      "implementation(\"io.awspring.cloud:spring-cloud-starter-aws-secrets-manager-config\")",
      "implementation 'io.awspring.cloud:spring-cloud-aws-secrets-manager-config'",
      "implementation(\"io.awspring.cloud:spring-cloud-aws-secrets-manager-config\")",
      "implementation 'io.awspring.cloud:spring-cloud-aws-starter-secrets-manager'",
      "implementation(\"io.awspring.cloud:spring-cloud-aws-starter-secrets-manager\")",
      "implementation 'io.awspring.cloud:spring-cloud-aws-secrets-manager'",
      "implementation(\"io.awspring.cloud:spring-cloud-aws-secrets-manager\")"
    ],
    "shouldNotMatch": [
      "implementation 'com.azure.spring:spring-cloud-azure-starter-keyvault-secrets'",
      "implementation(\"com.azure.spring:spring-cloud-azure-starter-keyvault-secrets\")",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
