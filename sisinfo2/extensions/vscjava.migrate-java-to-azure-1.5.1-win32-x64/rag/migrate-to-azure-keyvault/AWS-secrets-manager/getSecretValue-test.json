{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "GetSecretValueRequest request = GetSecretValueRequest.builder().secretId(\"example\").build();",
      "BatchGetSecretValueRequest request = BatchGetSecretValueRequest.builder().secretIdList(\"example1\", \"example2\").build();",
      "GetSecretValueRequest request = GetSecretValueRequest.builder().versionStage(\"AWSCURRENT\").build();"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "System.out.println(\"Unrelated code snippet\");"
    ]
  }
}
