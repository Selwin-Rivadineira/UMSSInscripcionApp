{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "Cipher cipher = Cipher.getInstance(\"AES\");",
      "Signature signature = Signature.getInstance(\"SHA256withRSA\");"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "int number = 42;"
    ]
  }
}
