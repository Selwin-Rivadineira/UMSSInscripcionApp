{
  "filePattern": {
    "shouldMatch": [
      "application.properties",
      "application.yml",
      "application.yaml",
      "logback.xml",
      "logging.properties",
      "log4j2.xml",
      "tinylog.properties",
      "config/logging.properties",
      "config/log4j2.xml",
      "custom/path/to/application.properties",
      "custom/path/to/logging.properties",
      "logback-test.xml",
      "config/log4j2.json",
      "custom/path/to/tinylog.properties"
    ],
    "shouldNotMatch": [
      "config.path=C:/Users/data/config/settings.xml",
      "log.path=/invalid/path/to/logs",
      "invalid/logging.config",
      "invalid/log4j2.config",
      "random/path/to/file.txt",
      "config/logging.yam",
      "config/log4j2.xml.bak",
      "random/path/to/invalid.config"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "file.path=C:/Users/data/logs/app.log",
      "file.path=/var/log/app.log",
      "file.name=logfile.log",
      "log.file=/tmp/logs/output.log",
      "log.file.name=error.log",
      "log.file.path=/custom/logs/custom.log",
      "log.file.path=/var/logs/debug.log",
      "log.file.path=/var/logs/app.log",
      "file.name=application.log",
      "file.path=/tmp/logs/debug.log"
    ],
    "shouldNotMatch": [
      "app.path=/path/to/logs",
      "config.path=C:/Users/data/config/settings.xml",
      "app.path=/logs/path",
      "config.path=/etc/config/settings.xml",
      "random.key=random.value"
    ]
  }
}
