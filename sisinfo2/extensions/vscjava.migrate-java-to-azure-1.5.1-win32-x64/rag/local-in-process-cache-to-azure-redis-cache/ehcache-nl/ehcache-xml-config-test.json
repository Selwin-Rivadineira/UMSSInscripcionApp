{
  "filePattern": {
    "shouldMatch": [
      "config/ehcache.xml",
      "nested/config/ehcache.xml",
      "project/ehcache.xml"
    ],
    "shouldNotMatch": [
      "config/ehcache.json",
      "nested/config/ehcache.yaml",
      "project/ehcache.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<ehcache>\n  <defaultCache maxElementsInMemory=\"1000\" eternal=\"false\"/>\n</ehcache>",
      "<cache name=\"exampleCache\" timeToLiveSeconds=\"3600\"/>",
      "<diskStore path=\"java.io.tmpdir\"/>",
      "<defaultCache memoryStoreEvictionPolicy=\"LRU\"/>"
    ],
    "shouldNotMatch": [
      "<configuration>\n  <property name=\"unrelatedProperty\" value=\"value\"/>\n</configuration>"
    ]
  }
}
