{
  "filePattern": {
    "shouldMatch": [
      "CacheManagerImpl.java",
      "nested/CacheManagerImpl.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "public class CustomCacheManager extends CacheManager {",
      "EhCacheCacheManager cacheManager = new EhCacheCacheManager();",
      "SimpleCacheManager simpleCacheManager = new SimpleCacheManager();",
      "ConcurrentMapCacheManager concurrentMapCacheManager = new ConcurrentMapCacheManager();",
      "public class MyCacheManager extends CacheManager {"
    ],
    "shouldNotMatch": [
      "@Transactional",
      "@Component",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
