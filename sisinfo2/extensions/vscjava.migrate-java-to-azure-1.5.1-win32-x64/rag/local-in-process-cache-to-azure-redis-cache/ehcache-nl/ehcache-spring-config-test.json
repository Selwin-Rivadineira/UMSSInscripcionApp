{
  "filePattern": {
    "shouldMatch": [
      "example/ConfigClass.java",
      "nested/example/ConfigClass.java",
      "project/CacheConfig.java"
    ],
    "shouldNotMatch": [
      "example/ConfigClass.kt",
      "nested/example/ConfigClass.txt",
      "project/CacheConfig.xml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "@EnableCaching",
      "public CacheManager cacheManager()",
      "return new EhCacheCacheManager()",
      "public EhCacheManagerFactoryBean ehCacheManagerFactoryBean() {\n    return new EhCacheManagerFactoryBean();\n  }\n}",
      "import net.sf.ehcache.Cache;\nimport net.sf.ehcache.CacheManager;"
    ],
    "shouldNotMatch": [
      "import org.springframework.data.redis.connection.lettuce.LettuceConnectionFactory;",
      "@Configuration\npublic class CacheConfig {\n  @Bean\n  public RedisTemplate redisTemplate() {\n    return new RedisTemplate();\n  }\n}"
    ]
  }
}
