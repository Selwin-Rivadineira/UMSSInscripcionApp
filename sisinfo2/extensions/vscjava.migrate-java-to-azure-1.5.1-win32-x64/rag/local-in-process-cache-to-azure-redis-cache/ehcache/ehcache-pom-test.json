{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "subdir/project/pom.xml"
    ],
    "shouldNotMatch": [
      "build.gradle",
      "nested/build.gradle",
      "subdir/project/build.gradle"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<dependency>\n  <groupId>net.sf.ehcache</groupId>\n  <artifactId>ehcache</artifactId>\n  <version>2.10.6</version>\n</dependency>",
      "<dependency>\n  <groupId>net.sf.ehcache</groupId>\n  <artifactId>ehcache-core</artifactId>\n  <version>2.6.11</version>\n</dependency>",
      "<dependency>\n  <groupId>org.springframework.boot</groupId>\n  <artifactId>spring-boot-starter-cache</artifactId>\n</dependency>",
      "<dependency>\n  <groupId>org.springframework.boot</groupId>\n  <groupId>net.sf.ehcache<\/groupId>\n</dependency>"
    ],
    "shouldNotMatch": [
      "<dependency>\n  <groupId>com.example</groupId>\n  <artifactId>unrelated-dependency</artifactId>\n  <version>1.0.0</version>\n</dependency>",
      "<dependency>\n  <groupId>org.apache.activemq</groupId>\n  <artifactId>activemq-core</artifactId>\n  <version>5.15.9</version>\n</dependency>"
    ]
  }
}
