{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "build.gradle",
      "nested/build.gradle"
    ],
    "shouldNotMatch": [
      "Application.java",
      "application.properties",
      "config.yaml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>mssql-jdbc</artifactId>",
      "implementation 'com.microsoft.sqlserver:mssql-jdbc:10.2.0.jre11'"
    ],
    "shouldNotMatch": [
      "<artifactId>azure-storage-queue</artifactId>",
      "testImplementation 'com.azure:azure-data-tables:12.5.0'"
    ]
  }
}
