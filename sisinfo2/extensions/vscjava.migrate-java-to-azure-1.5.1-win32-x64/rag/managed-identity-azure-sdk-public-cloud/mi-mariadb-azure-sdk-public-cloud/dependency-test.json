{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "build.gradle",
      "nested/build.gradle"
    ],
    "shouldNotMatch": [
      "Application.java",
      "application.properties",
      "config.yaml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>mariadb-java-client</artifactId>",
      "implementation 'org.mariadb.jdbc:mariadb-java-client:3.1.4'",
      "implementation group: 'org.mariadb.jdbc', name: 'mariadb-java-client', version: '3.1.4'",
      "<dependency>\n        <groupId>org.mariadb.jdbc</groupId>\n        <artifactId>mariadb-java-client</artifactId>\n        <version>3.0.6</version>\n      </dependency>",
      "compile 'org.mariadb:mariadb-connector-j:3.1.4'",
      "runtime 'org.mariadb.jdbc:mariadb-java-client:2.7.5'"
    ],
      "shouldNotMatch": [
      "<artifactId>azure-storage-queue</artifactId>",
      "testImplementation 'com.azure:azure-data-tables:12.5.0'",
      "<artifactId>mongodb-driver-sync</artifactId>",
      "implementation 'org.springframework.boot:spring-boot-starter-data-jpa:2.7.0'",
      "implementation group: 'com.azure', name: 'azure-cosmos', version: '4.45.1'",
      "<dependency>\n        <groupId>org.postgresql</groupId>\n        <artifactId>postgresql</artifactId>\n        <version>42.5.1</version>\n      </dependency>",
      "compile 'redis.clients:jedis:4.3.1'",
      "implementation 'org.neo4j:neo4j-ogm-core:3.2.37'"
    ]
  }
}
