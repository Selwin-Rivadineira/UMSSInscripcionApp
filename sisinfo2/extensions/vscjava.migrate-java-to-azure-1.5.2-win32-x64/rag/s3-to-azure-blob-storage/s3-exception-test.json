{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "BucketAlreadyExistsException exception = new BucketAlreadyExistsException();",
      "InvalidWriteOffsetException exception = new InvalidWriteOffsetException();",
      "NoSuchBucketException exception = new NoSuchBucketException();",
      "NoSuchKeyException exception = new NoSuchKeyException();"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
