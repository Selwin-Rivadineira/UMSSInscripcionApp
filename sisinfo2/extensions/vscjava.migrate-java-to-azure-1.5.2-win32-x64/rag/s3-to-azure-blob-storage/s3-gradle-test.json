{
  "filePattern": {
    "shouldMatch": [
      "build.gradle",
      "nested/build.gradle",
      "subdir/build.gradle"
    ],
    "shouldNotMatch": [
      "build.xml",
      "nested/build.txt",
      "subdir/build.gradle.kts"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "implementation 'software.amazon.awssdk:s3:2.17.89'",
      "implementation 'io.awspring.cloud:spring-cloud-aws-starter-s3:2.3.0'",
      "implementation 'com.amazonaws:aws-java-sdk-s3:1.11.1000'"
    ],
    "shouldNotMatch": [
      "implementation 'com.azure:azure-storage-blob:12.14.0'",
      "implementation 'com.azure:azure-identity:1.3.0'",
      "implementation 'com.azure:azure-storage-blob-batch:12.14.0'"
    ]
  }
}
