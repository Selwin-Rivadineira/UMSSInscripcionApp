{
  "filePattern": {
    "shouldMatch": [
      "application.properties",
      "application.yaml",
      "application.yml",
      "config/application.properties",
      "nested/config/application.yaml"
    ],
    "shouldNotMatch": [
      "Main.java",
      "settings.json",
      "config/other.properties",
      "application.txt",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "s3.bucket.name=mybucket",
      "bucket=mybucket",
      "key=mykey",
      "aws.s3.location=us-west-2",
      "spring.s3.key=secretkey"
    ],
    "shouldNotMatch": [
      "database.url=jdbc:mysql://localhost:3306/db",
      "server.port=8080",
      "azure.blob.container=container1",
      "logging.level=INFO"
    ]
  }
}
