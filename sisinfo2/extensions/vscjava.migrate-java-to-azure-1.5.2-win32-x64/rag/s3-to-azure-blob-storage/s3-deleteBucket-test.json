{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "DeleteBucketRequest req = DeleteBucketRequest.builder().bucket(\"myBucket\").build();",
      "DeleteBucketResponse res = s3Client.deleteBucket(req);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
