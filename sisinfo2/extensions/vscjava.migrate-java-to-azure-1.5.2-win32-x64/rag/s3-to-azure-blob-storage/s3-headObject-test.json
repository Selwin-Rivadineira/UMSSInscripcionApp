{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "HeadObjectRequest request = HeadObjectRequest.builder().bucket(\"myBucket\").build();",
      "HeadObjectResponse response = s3Client.headObject(request);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobClient blobClient = new BlobClient();"
    ]
  }
}
