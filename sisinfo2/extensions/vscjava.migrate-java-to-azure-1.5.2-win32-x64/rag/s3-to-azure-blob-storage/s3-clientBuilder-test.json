{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "S3Client client = S3Client.builder().build();",
      "S3ClientBuilder builder = S3Client.builder();"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobServiceClient blobServiceClient = new BlobServiceClientBuilder().buildClient();"
    ]
  }
}
