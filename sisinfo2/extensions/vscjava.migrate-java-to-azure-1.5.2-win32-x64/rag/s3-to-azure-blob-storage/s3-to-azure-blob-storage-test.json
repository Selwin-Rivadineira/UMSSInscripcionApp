{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "import com.amazonaws.services.s3.AmazonS3Client;",
      "import software.amazon.awssdk.services.s3.S3Client;"
    ],
    "shouldNotMatch": [
      "import com.azure.storage.blob.models.BlobClient;",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
