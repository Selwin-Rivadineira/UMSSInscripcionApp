{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "DeleteObjectRequest request = DeleteObjectRequest.builder().bucket(bucketName).key(objectKey).build();",
      "DeleteObjectsRequest request = DeleteObjectsRequest.builder().bucket(bucketName).delete(delete).build();",
      "DeleteObjectResponse response = s3Client.deleteObject(request);",
      "DeleteObjectsResponse response = s3Client.deleteObjects(request);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobClient blobClient = new BlobClient();"
    ]
  }
}
