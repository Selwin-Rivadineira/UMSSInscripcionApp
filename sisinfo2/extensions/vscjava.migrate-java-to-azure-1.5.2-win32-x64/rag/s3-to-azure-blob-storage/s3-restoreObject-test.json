{
  "filePattern": {
    "shouldMatch": [
      "RestoreObjectRequest.java",
      "nested/RestoreObjectRequest.java",
      "RestoreObjectResponse.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/RestoreObjectRequest.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "RestoreObjectRequest request = RestoreObjectRequest.builder().bucket(\"myBucket\").key(\"myKey\").build();",
      "RestoreObjectResponse response = s3Client.restoreObject(request);",
      "RestoreObjectRequest.Builder builder = RestoreObjectRequest.builder();",
      "RestoreObjectResponse.Builder builder = RestoreObjectResponse.builder();"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "BlobClient blobClient = new BlobClient();"
    ]
  }
}
