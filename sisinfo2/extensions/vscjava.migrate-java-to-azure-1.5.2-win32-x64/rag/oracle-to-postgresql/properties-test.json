{
  "filePattern": {
    "shouldMatch": [
      "application.properties",
      "config.yml",
      "settings.yaml"
    ],
    "shouldNotMatch": [
      "src/Main.jav",
      "src/Main.txt",
      "src/Main.java.bak",
      "src/Main.java1",
      "src/Main.javac",
      "src/Main.py",
      "src/Main.java/extra",
      "src/.java",
      "src/Main.JAVA",
      "src/com/example/util/Helper.java",
      "src/database/OracleMigration.java",
      "src/database/OracleMigration.bak",
      "src/database/PostgreSQLMigration.bak",
      "src/com/example/orm/EntityMapping.bak",
      "src/com/example/orm/DatabaseConfig.bak"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "spring.datasource.url=jdbc:oracle:thin:@localhost:1521:ORCL"
    ],
    "shouldNotMatch": [
      "aaa=bbb"
    ]
  }
}
