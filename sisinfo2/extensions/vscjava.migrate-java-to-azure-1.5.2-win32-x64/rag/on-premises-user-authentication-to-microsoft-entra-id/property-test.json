{
  "filePattern": {
    "shouldMatch": [
      "application.yml",
      "application.yaml",
      "application.properties",
      "application-dev.yml",
      "application-prod.properties",
      "config/application.yml",
      "config/application.properties",
      "src/main/resources/application.properties",
      "src/main/resources/config/application-dev.yml",
      "src/main/resources/config/application-prod.yml",
      "src/main/resources/config/application-test.properties"
    ],
    "shouldNotMatch": [
      "application.txt",
      "application.xml",
      "application.json",
      "config/application.xml",
      "config/application.json",
      "src/main/resources/application.json",
      "src/main/resources/config/application-dev.txt",
      "src/main/resources/config/application-prod.json",
      "src/main/resources/config/application-test.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "spring.cloud.azure.active-directory.enabled: true",
      "spring.cloud.azure.active-directory.profile.tenant-id: ${AZURE_TENANT_ID}",
      "spring.cloud.azure.active-directory.credential.client-id: ${AZURE_CLIENT_ID}",
      "spring.cloud.azure.active-directory.credential.client-secret: ${AZURE_CLIENT_SECRET}",
      "spring.cloud.azure.active-directory.profile.group: ${AZURE_GROUP_ID}",
      "spring.cloud.azure.active-directory.profile.environment: ${AZURE_ENVIRONMENT}"
    ],
    "shouldNotMatch": [
    ]
  }
}
