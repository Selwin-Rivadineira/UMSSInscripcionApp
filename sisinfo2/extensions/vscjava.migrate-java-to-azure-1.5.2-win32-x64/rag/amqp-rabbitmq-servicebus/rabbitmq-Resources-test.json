{
  "filePattern": {
    "shouldMatch": [
      "RabbitMQConfig.java",
      "src/main/java/com/example/config/AmqpConfig.java",
      "nested/folder/QueueConfig.java",
      "com/myapp/amqp/ExchangeConfig.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "RabbitMQConfig.kt",
      "src/main/resources/application.properties",
      "config/rabbitmq.yml", 
      "RabbitQueue.html",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "import org.springframework.amqp.core.Queue;",
      "import org.springframework.amqp.core.Binding;",
      "import org.springframework.amqp.core.BindingBuilder;",
      "import org.springframework.amqp.core.TopicExchange;",
      "import org.springframework.amqp.core.DirectExchange;",
      "org.springframework.amqp.core.AmqpAdmin amqpAdmin = new RabbitAdmin(connectionFactory);"
    ],
    "shouldNotMatch": [
      "import com.azure.messaging.servicebus.administration.ServiceBusAdministrationClient;",
      "import com.azure.messaging.servicebus.administration.models.TopicProperties;",
      "@Bean\npublic TopicProperties topicProperties(ServiceBusAdministrationClient adminClient) {\n    return adminClient.createTopic(\"myTopic\");\n}",
      "import org.springframework.amqp.rabbit.core.RabbitTemplate;",
      "import com.rabbitmq.client.Channel;",
      "String rabbitMQHost = \"localhost\";"
    ]
  }
}
