{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/folder/pom.xml",
      "project/build.gradle",
      "src/main/build.gradle",
      "src/main/build.gradle.kts",
      "multi-module/module1/pom.xml"
    ],
    "shouldNotMatch": [
      "build.xml",
      "pom.yaml",
      "src/main/resources/application.properties",
      "settings.gradle",
      "package.json"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<dependency>\n\t<groupId>org.springframework.boot</groupId>\n\t<artifactId>spring-boot-starter-amqp</artifactId>\n</dependency>",
      "<dependency>\n\t<groupId>org.springframework.amqp</groupId>\n\t<artifactId>spring-rabbit</artifactId>\n\t<version>2.4.7</version>\n</dependency>",
      "implementation 'org.springframework.boot:spring-boot-starter-amqp'",
      "implementation 'org.springframework.amqp:spring-amqp:2.4.7'",
      "testImplementation 'org.springframework.amqp:spring-rabbit-test'",
      "<artifactId>spring-rabbit</artifactId>",
      "<artifactId>spring-amqp</artifactId>"
    ],
    "shouldNotMatch": [
      "<dependency>\n\t<groupId>com.azure.spring</groupId>\n\t<artifactId>spring-cloud-azure-starter</artifactId>\n</dependency>",
      "implementation 'com.azure.spring:spring-messaging-azure-servicebus'",
      "<dependency>\n\t<groupId>org.springframework.boot</groupId>\n\t<artifactId>spring-boot-starter-web</artifactId>\n</dependency>",
      "implementation 'org.springframework.boot:spring-boot-starter-data-jpa'",
      "<artifactId>spring-kafka</artifactId>",
      "compile group: 'com.rabbitmq', name: 'amqp-client', version: '5.14.2'"
    ]
  }
}
