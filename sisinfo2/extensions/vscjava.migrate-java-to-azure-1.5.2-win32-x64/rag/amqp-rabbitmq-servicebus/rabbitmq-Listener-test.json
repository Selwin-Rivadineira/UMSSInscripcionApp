{
  "filePattern": {
    "shouldMatch": [
      "RabbitListenerConfig.java",
      "src/main/java/com/example/listener/MessageListener.java",
      "src/main/java/com/example/config/RabbitConfig.java",
      "nested/folder/QueueListener.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "RabbitConfig.js",
      "src/main/resources/application.properties",
      "src/main/kotlin/RabbitConfig.kt",
      "config/rabbitmq.yml",
      "test.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "@EnableRabbit\npublic class RabbitConfig {",
      "@RabbitListener(queues = \"myQueue\")\npublic void processMessage(String message) {",
      "@RabbitListener(bindings = @QueueBinding(\n    value = @Queue(\"orders\"),\n    exchange = @Exchange(\"order-exchange\")))",
      "@RabbitHandler\npublic void handleMessage(Order order) {",
      "import org.springframework.amqp.rabbit.annotation.EnableRabbit;",
      "import org.springframework.amqp.rabbit.annotation.RabbitListener;",
      "import org.springframework.amqp.rabbit.annotation.RabbitHandler;"
    ],
    "shouldNotMatch": [
      "import org.springframework.jms.annotation.JmsListener;",
      "@JmsListener(destination = \"myQueue\")",
      "import com.azure.messaging.servicebus.ServiceBusClientBuilder;",
      "import com.azure.spring.messaging.servicebus.implementation.core.annotation.ServiceBusListener;",
      "@ServiceBusListener(destination = \"myTopic\", group = \"mySubscription\")",
      "String rabbitMQHost = \"localhost\";"
    ]
  }
}
