{
  "filePattern": {
    "shouldMatch": [
      "RabbitMQContainerTest.java",
      "src/test/java/com/example/test/RabbitIntegrationTest.java",
      "nested/folder/TestConfiguration.java",
      "com/myapp/test/RabbitTestSetup.java",
      "integration/RabbitMQSetup.java"
    ],
    "shouldNotMatch": [
      "RabbitMQTest.kt",
      "src/main/resources/application-test.properties",
      "config/test.yml", 
      "RabbitMQContainer.html",
      "README.md"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "import org.testcontainers.containers.RabbitMQContainer;",
      "private static final RabbitMQContainer rabbitMQContainer = new RabbitMQContainer(\"rabbitmq:3.9-management\");",
      "@Container\nRabbitMQContainer container = new RabbitMQContainer();",
      "RabbitMQContainer.ExecResult result = rabbitMQContainer.execInContainer(\"rabbitmqctl\", \"list_queues\");",
      "RabbitMQContainer rabbitMQContainer = new RabbitMQContainer(\"rabbitmq:3.8.3\")\n    .withExposedPorts(5672, 15672);",
      "static {\n    rabbitMQContainer.start();\n}",
      "CachingConnectionFactory connectionFactory = new CachingConnectionFactory(rabbitMQContainer.getHost(), rabbitMQContainer.getAmqpPort());"
    ],
    "shouldNotMatch": [
      "import com.azure.messaging.servicebus.ServiceBusClientBuilder;",
      "ServiceBusClientBuilder builder = new ServiceBusClientBuilder();",
      "import org.testcontainers.containers.PostgreSQLContainer;",
      "PostgreSQLContainer<?> postgres = new PostgreSQLContainer<>(\"postgres:13\");",
      "String connectionString = System.getenv(\"SERVICEBUS_CONNECTION_STRING\");",
      "import org.testcontainers.containers.GenericContainer;"
    ]
  }
}
