{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "CreateSecretRequest request = CreateSecretRequest.builder().name(\"MySecret\").build();",
      "PutSecretValueRequest request = PutSecretValueRequest.builder().secretId(\"MySecretId\").build();"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "public class AnotherClass {}"
    ]
  }
}
