{
  "filePattern": {
    "shouldMatch": [
      "src/Main.java",
      "com/example/MyClass.java",
      "test/unit/ServiceTest.java",
      "org/company/project/Utils.java",
      "Main.java",
      "src/com/example/util/Helper.java",
      "src/com/example/AnotherClass.java",
      "test/integration/IntegrationTest.java",
      "src/main/java/com/example/App.java",
      "src/test/java/com/example/TestApp.java",
      "src/org/company/project/Main.java"
    ],
    "shouldNotMatch": [
      "src/Main.jav",
      "src/Main.txt",
      "src/Main.java.bak",
      "src/Main.java1",
      "src/Main.javac",
      "src/Main.py",
      "src/Main.java/extra",
      "src/.java",
      "src/Main.JAVA",
      "src/README.md",
      "src/main/java/com/example/App.jav",
      "src/main/java/com/example/App.txt",
      "src/main/java/com/example/App.java.bak"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "FileAppender",
      "FileWriter",
      "FileHandler",
      "SocketHandler",
      "FileAppender appender = new FileAppender();",
      "FileWriter writer = new FileWriter(file);",
      "FileHandler handler = new FileHandler();"
    ],
    "shouldNotMatch": [
      "BufferedWriter",
      "System.out.println(\"Logging to console\");",
      "String path = \"/WEB-INF/config.xml\";",
      "String path = \"/src/main/resources/config.properties\";",
      "String url = \"https://example.com/api/v1\";",
      "String url = \"http://example.com/api/v1\";",
      "String[] urls = {\"http://example.com/api/v1\", \"https://example.org/api/v2\"};",
      "BufferedReader reader = new BufferedReader(new FileReader(file));",
      "System.out.println(\"This is a test log\");",
      "String configPath = \"/config/settings.xml\";"
    ]
  }
}
