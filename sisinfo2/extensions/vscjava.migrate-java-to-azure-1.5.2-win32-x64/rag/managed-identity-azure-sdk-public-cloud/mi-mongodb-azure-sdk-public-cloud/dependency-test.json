{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "nested/pom.xml",
      "build.gradle",
      "nested/build.gradle",
      "build.gradle.kts",
      "nested/build.gradle.kts"
    ],
    "shouldNotMatch": [
      "Application.java",
      "application.properties",
      "config.yaml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<artifactId>mongodb-driver-core</artifactId>",
      "<artifactId>mongodb-driver-sync</artifactId>",
      "implementation 'org.mongodb:mongodb-driver-sync:4.9.1'",
      "implementation group: 'org.mongodb', name: 'mongodb-driver-core', version: '4.9.1'",
      "<dependency>\n        <groupId>org.mongodb</groupId>\n        <artifactId>mongo-java-driver</artifactId>\n        <version>3.12.14</version>\n      </dependency>",
      "compile 'org.mongodb:mongodb-driver:3.12.14'",
      "implementation 'org.mongodb.morphia:morphia:1.3.2'",
      "implementation 'org.springframework.boot:spring-boot-starter-data-mongodb:3.1.0'"
    ],
      "shouldNotMatch": [
      "<artifactId>azure-storage-queue</artifactId>",
      "testImplementation 'com.azure:azure-data-tables:12.5.0'",
      "implementation 'org.springframework.boot:spring-boot-starter-data-jpa:2.7.0'",
      "implementation group: 'com.azure', name: 'azure-cosmos', version: '4.45.1'",
      "<dependency>\n        <groupId>org.postgresql</groupId>\n        <artifactId>postgresql</artifactId>\n        <version>42.5.1</version>\n      </dependency>",
      "compile 'redis.clients:jedis:4.3.1'",
      "implementation 'org.neo4j:neo4j-ogm-core:3.2.37'"
    ]
  }
}
