{
  "filePattern": {
    "shouldMatch": [
      "example/pom.xml",
      "nested/example/pom.xml",
      "project/pom.xml"
    ],
    "shouldNotMatch": [
      "example/build.gradle",
      "nested/example/build.gradle",
      "project/settings.gradle"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<dependency>\n  <groupId>net.sf.ehcache</groupId>",
      "<dependency>\n  <artifactId>ehcache-core</artifactId>\n</dependency>",
      "<dependency>\n  <artifactId>spring-boot-starter-cache</artifactId>\n</dependency>",
      "<dependency>\n  <artifactId>ehcache</artifactId>\n</dependency>"
    ],
    "shouldNotMatch": [
      "<dependency>\n  <groupId>com.example</groupId>\n  <artifactId>unrelated-dependency</artifactId>\n</dependency>",
      "<dependency>\n  <groupId>org.springframework.boot</groupId>\n  <artifactId>spring-boot-starter-data-redis</artifactId>\n</dependency>"
    ]
  }
}
