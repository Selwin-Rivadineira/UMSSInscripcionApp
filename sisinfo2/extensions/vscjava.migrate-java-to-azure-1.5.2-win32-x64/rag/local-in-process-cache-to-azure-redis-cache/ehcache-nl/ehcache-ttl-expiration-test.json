{
  "filePattern": {
    "shouldMatch": [
      "example/CacheConfig.java",
      "nested/example/CacheConfig.java",
      "project/CacheManager.java"
    ],
    "shouldNotMatch": [
      "example/CacheConfig.kt",
      "nested/example/CacheConfig.txt",
      "project/CacheManager.xml"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "cacheConfig.timeToLiveSeconds(3600);",
      "timeToIdleSeconds(1800);",
      "cacheConfig.setTimeToIdle(1800);",
      "element.setTimeToLive(3600);",
      "element.setEternal(false);",
      "diskExpiryThreadIntervalSeconds(120);",
      "eternal=false;"
    ],
    "shouldNotMatch": [
      "cacheConfig.unrelatedMethod(1234);",
      "element.setUnrelatedProperty(true);"
    ]
  }
}
