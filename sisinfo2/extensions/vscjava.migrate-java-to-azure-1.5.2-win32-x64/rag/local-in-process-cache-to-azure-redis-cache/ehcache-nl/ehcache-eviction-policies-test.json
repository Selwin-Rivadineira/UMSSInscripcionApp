{
  "filePattern": {
    "shouldMatch": [
      "ExampleClass.java",
      "nested/ExampleClass.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "memoryStoreEvictionPolicy",
      "MemoryStoreEvictionPolicy policy = MemoryStoreEvictionPolicy.LRU;",
      "MemoryStoreEvictionPolicy policy = MemoryStoreEvictionPolicy.LFU;",
      "MemoryStoreEvictionPolicy policy = MemoryStoreEvictionPolicy.FIFO;",
      "maxElementsInMemory(100);",
      "maxBytesLocalHeap(1024 * 1024);"
    ],
    "shouldNotMatch": [
      "String unrelated = \"UnrelatedClass\";",
      "cache.setMaxElementsInDisk(1000);"
    ]
  }
}
