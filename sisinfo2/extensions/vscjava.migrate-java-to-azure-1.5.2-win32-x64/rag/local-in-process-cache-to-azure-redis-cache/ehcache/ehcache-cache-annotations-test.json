{
  "filePattern": {
    "shouldMatch": [
      "UserService.java",
      "nested/UserService.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "@Cacheable(key = \"#id\")",
      "@CachePut(key = \"#user.id\")",
      "@CacheEvict(key = \"#id\")",
      "@Caching(evict = {\n  @CacheEvict(value = \"users\", key = \"#id\"),\n  @CacheEvict(value = \"userProfiles\", key = \"#id\")\n})",
      "@CacheConfig(cacheNames = \"users\")"
    ],
    "shouldNotMatch": [
      "Cacheable(key = \"#unrelated\")",
      "String unrelated = \"UnrelatedClass\";"
    ]
  }
}
