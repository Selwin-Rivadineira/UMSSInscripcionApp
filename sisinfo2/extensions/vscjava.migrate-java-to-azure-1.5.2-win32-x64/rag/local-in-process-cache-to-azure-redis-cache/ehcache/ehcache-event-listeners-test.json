{
  "filePattern": {
    "shouldMatch": [
      "CacheEventListener.java",
      "nested/CacheEventListener.java",
      "Main.java"
    ],
    "shouldNotMatch": [
      "AnotherClass.kt",
      "nested/ExampleClass.txt"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "public class CustomCacheEventListener implements CacheEventListener {",
      "public class CacheLogger extends CacheEventListener {",
      "@CacheEventListener",
      "@EventType(EventType.CREATED)",
      "CacheEntryEvent<? extends String, ? extends String> event",
      "cache.getCacheEventNotificationService().registerCacheEventListener(listener);",
      "CacheEntryCreateEvent",
      "CacheEntryUpdatedEvent",
      "CacheEntryRemovedEvent",
      "CacheEntryExpiredEvent"
    ],
    "shouldNotMatch": [
      "public class UnrelatedClass {",
      "String unrelated = \"UnrelatedClass\";",
      "@UnrelatedAnnotation"
    ]
  }
}
