{
  "filePattern": {
    "shouldMatch": [
      "pom.xml",
      "build.gradle",
      "build.gradle.kts",
      "src/main/pom.xml",
      "project/build.gradle",
      "services/kafka-service/build.gradle.kts"
    ],
    "shouldNotMatch": [
      "settings.gradle",
      "application.properties",
      "src/main/resources/application.yml",
      "src/main/java/com/example/KafkaConfig.java",
      "README.md",
      "build.gradlex"
    ]
  },
  "codePattern": {
    "shouldMatch": [
      "<dependency>\n  <groupId>org.springframework.kafka</groupId>\n  <artifactId>spring-kafka</artifactId>\n  <version>2.8.0</version>\n</dependency>",
      "implementation 'org.springframework.boot:spring-boot-starter-kafka:2.7.0'",
      "implementation(\"org.springframework.kafka:spring-kafka-streams:3.0.0\")",
      "compile group: 'org.springframework.integration', name: 'spring-integration-kafka', version: '5.5.0'",
      "implementation 'org.springframework.cloud:spring-cloud-stream-binder-kafka:3.2.4'",
      "implementation \"org.springframework.cloud:spring-cloud-starter-stream-kafka\"",
      "api 'org.springframework.cloud:spring-cloud-stream-binder-kafka-streams:3.2.0'"
    ],
    "shouldNotMatch": [
      "<dependency>\n  <groupId>org.apache.kafka</groupId>\n  <artifactId>kafka-clients</artifactId>\n  <version>3.3.1</version>\n</dependency>",
      "implementation 'com.azure.spring:spring-cloud-azure-starter:5.5.0'",
      "implementation 'org.springframework.boot:spring-boot-starter-web'"
    ]
  }
}
